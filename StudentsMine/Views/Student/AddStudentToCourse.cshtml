@model List<StudentsMine.Models.OrderStudentToCourceView>
@{
    ViewBag.Title = "Register";
}
<script src="/Scripts/duplicator.js"> </script>
<script src="/Scripts/createStudent.js"> </script>
<style>
    .create-students-block {
        background-color:white;
        width: 1125px;
        padding: 10px;
        margin: 10px;
        box-shadow: 0 0.1rem 0.2rem rgba(0,0,0,0.12), 0 0 0.1rem rgba(0,0,0,0.12);
        height: 550px;
        background-color:white;
        overflow: auto;
    }
    .dublicate-data-group {
        display: inline-block;
        width: 350px;
        padding: 10px;
        margin: 5px;
        box-shadow: 8px 9px 20px 0px #6960602e;
        border-radius: 3px;
    }
    .error-message {
        text-align: center;
    }
</style>

<div style=" top: 50%; transform: translateY(-50%); position: relative;">
    <div class="form-horizontal create-students-block">

    @Html.AntiForgeryToken()
    <div class="dublicate-data-group" index='0'>
        <input name="[0].email%UIId" class="data-field data UIId" hidden />
        <div class="form-group">
            <label class="col-md-2 control-label">
                Email
            </label>
            <div class="col-md-10">
                <input type="text" name="[0].email%Email" class="form-control data-field data" />
            </div>
        </div>
        <div class="error-message">

        </div>
    </div>
        </div>
    <div class="btn btn-default" onclick='createStudents(@ViewData["courseId"] , "AddStudentToCourse")'>Add to course</div>
    </div>